[%# JSON format autocomplete response %]
[%  IF (! ac_field.defined );
        THROW rdgc '"ac_field" must be defined';
    END;
    IF (! ac_method.defined );
        THROW rdgc '"ac_method" must be defined';
    END;
    
    SET dataset = [];
    SET no_wrapper = 1;
    SET count = 0;
    FOR r = results;
        SET pk = c.controller.make_primary_key_string(r);
        dataset.push({ $ac_field => r.$ac_method, pk => $pk });
        count = count + 1;
    END;
    resultset = {
        'ResultSet' => {
            'Count'  => count,
            'Result' => dataset
        }
    };
    resultset.as_json;
%]
